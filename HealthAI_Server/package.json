{
  "name": "health-ai-server",
  "version": "1.0.0",
  "description": "API Server cho dự án AI Y tế với Node.js, MySQL, và TensorFlow.js",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/",
    "db:check": "node check_db_structure.js",
    "db:migrate": "node run_migrations.js",
    "db:seed": "node run_seed.js",
    "db:init": "node run_migrations.js && node run_seed.js"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "firebase-admin": "^13.6.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mqtt": "^5.14.1",
    "multer": "^2.0.2",
    "mysql2": "^3.9.7",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "streamifier": "^0.1.1",
    "xlsx": "^0.18.5"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "services/**/*.js",
      "controllers/**/*.js",
      "routes/**/*.js",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "socket.io-client": "^4.8.3",
    "supertest": "^6.3.4"
  }
}